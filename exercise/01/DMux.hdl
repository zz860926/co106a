/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    Not(in=sel, out=Sel);
    Nand(a=in, b=Sel, out=x);
    Not(in=x, out=a);
    Nand(a=in, b=sel, out=y);
    Not(in=y, out=b);

}
