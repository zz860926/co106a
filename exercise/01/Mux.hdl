/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=Sel);
    Nand(a=a, b=Sel, out=x);    
    Nand(a=b, b=sel, out=y);
    Nand(a=x, b=y, out=out);
}